# Discord Bot Configuration
# Rename this file to .env and fill in your actual values

# Required: Your Discord Bot Token (from Discord Developer Portal)
TOKEN=your_discord_bot_token_here

# Required: Your Bot's Client ID (from Discord Developer Portal)
CLIENT_ID=your_bot_client_id_here

# Optional: Guild ID for testing (remove for global commands)
GUILD_ID=your_test_server_id_here

# Optional: Auto-assign this role to new members
AUTO_ROLE_ID=

# Optional: Role given after verification
VERIFY_ROLE_ID=

# Optional: Minutes before kicking unverified users (default: 10)
VERIFY_TIMEOUT_MINUTES=10

# Optional: Channel for welcome messages
WELCOME_CHANNEL_ID=

# Optional: Channel for goodbye messages
GOODBYE_CHANNEL_ID=

# Optional: Channel for moderation logs
LOGGING_CHANNEL_ID=